# + -/* /
print (20+3)
print(type(20+3))
print (20-3)
print(type(20-3))
print (20*3)
print(type(20*3))
print (20/3)
print(type(20/3))
print(int(20/3))
print(type(int(20/3)))
# // ** %
print(20//3)
print(type(20//3))
print(float(20//3))
print(type(20//3))
print(20**3)
print(type(20**3))
print(float(20**3))
print(20%3)
print(type(20%3))
print(float(20//3))
